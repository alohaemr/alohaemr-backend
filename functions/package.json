{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint .",
    "init": "firebase init emulators",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "deploy-by-name": "firebase deploy --only functions:name,functions:name",
    "logs": "firebase functions:log",
    "emulator-start": "firebase emulators:start --import ./firebase-export --export-on-exit ./firebase-export"
  },
  "engines": {
    "node": "16"
  },
  "main": "index.js",
  "dependencies": {
    "axios": "^0.26.1",
    "dotenv": "^16.0.3",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.24.1",
    "lodash": "^4.17.21",
    "twilio": "^3.82.1"
  },
  "devDependencies": {
    "eslint": "8.22.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^0.2.0"
  },
  "private": true
}
